<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>成功</title>
    <style>
        * {
            margin: 0;
            padding: 0;
        }

        html, body {
            height: 100%;
        }

        .transit {
            width: 100%;
            height: 100%;
            text-align: center;
            display: table;
        }

        .transit div {
            display: table-cell;
            vertical-align: middle;
        }

        h2 {
            margin: 20px 0;
        }
    </style>
</head>
<body>
<div class="transit">
    <div>
        <img src='data:image/png;base64,'/>
        <h2>{$data.message}</h2>
        <p>{$data.describe}</p>
        <p></p>
        <p>将在 <span id="mes">{$data.second}</span> 秒后进行跳转！</p>
    </div>
</div>
</body>
<script type="text/javascript">
    var i = {$data.second} -1;
    var url = "{$data.url}";
    var intervalid;
    intervalid = setInterval("fun()", 1000);

    function fun() {
        if (i == 0) {
            if (url == '') {
                window.history.back(-1);
            } else {
                location.href = url;
            }
            clearInterval(intervalid);
        }
        document.getElementById("mes").innerHTML = i;
        i--;
    }
</script>
</html>